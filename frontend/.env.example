# Roomie Connect Environment Variables
# Copy this file to .env and fill in your actual API keys

# Gemini AI API Key - Get from Google AI Studio
REACT_APP_GEMINI_API_KEY=your_gemini_api_key_here

# Firebase Configuration - Get from Firebase Console
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
REACT_APP_FIREBASE_DATABASE_URL=https://your_project-default-rtdb.firebaseio.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id

# Auth0 Configuration - Get from Auth0 Dashboard
AUTH0_DOMAIN=your_auth0_domain.us.auth0.com
AUTH0_CLIENT_ID=your_auth0_client_id
AUTH0_CLIENT_SECRET=your_auth0_client_secret

# Optional: Distance API Keys (currently using free alternatives)
# REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
# REACT_APP_OPENROUTE_API_KEY=your_openroute_api_key
